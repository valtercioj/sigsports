{
  "name": "sig",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "lint:deprecated": "eslint --no-eslintrc --config .eslintrc.deprecation.cjs --ext .ts,.tsx src",
    "deps:check": "ncu",
    "deps:update": "ncu -u",
    "deps:update-all": "ncu -u --target latest",
    "deps:doctor": "ncu --doctor --doctorTest 'npm run build'",
    "uml:generate": "tplant --input src/**/*.ts src/**/*.tsx --output docs/diagrams/class-diagram.puml",
    "uml:png": "tplant --input src/**/*.ts src/**/*.tsx --output docs/diagrams/class-diagram.png -A",
    "duplicate:check": "node check-duplicates.js"
  },
  "dependencies": {
    "@ant-design/plots": "^1.2.5",
    "@hookform/resolvers": "^3.3.0",
